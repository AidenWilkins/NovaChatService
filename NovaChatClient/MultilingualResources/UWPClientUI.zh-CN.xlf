<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en-US" target-language="zh-CN" original="UWPCLIENTUI/LOCALIZATIONS/EN-US/RESOURCES.RESW" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6916.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="UWPCLIENTUI/LOCALIZATIONS/EN-US/RESOURCES.RESW" datatype="resx">
        <trans-unit id="ConnectButton.Content" translate="yes" xml:space="preserve">
          <source>Connect</source>
          <target state="translated" state-qualifier="tm-suggestion">连接</target>
        </trans-unit>
        <trans-unit id="IPInput.PlaceholderText" translate="yes" xml:space="preserve">
          <source>IP Address</source>
          <target state="translated" state-qualifier="tm-suggestion">IP 地址</target>
        </trans-unit>
        <trans-unit id="LoginDescription.Text" translate="yes" xml:space="preserve">
          <source>Create a username and enter the address of the server you want to connect to.</source>
          <target state="translated">创造一位用户名然后输入一个IP地址.</target>
        </trans-unit>
        <trans-unit id="LoginTitle.Text" translate="yes" xml:space="preserve">
          <source>Nova Chat Client</source>
          <target state="translated">Nova 聊天客户</target>
        </trans-unit>
        <trans-unit id="PasswordInput.PlaceholderText" translate="yes" xml:space="preserve">
          <source>Password</source>
          <target state="translated" state-qualifier="tm-suggestion">密码</target>
        </trans-unit>
        <trans-unit id="UsernameInput.PlaceholderText" translate="yes" xml:space="preserve">
          <source>Username</source>
          <target state="translated" state-qualifier="tm-suggestion">用户名</target>
        </trans-unit>
        <trans-unit id="AdminPasswordInput.PlaceholderText" translate="yes" xml:space="preserve">
          <source>Admin Password</source>
          <target state="translated">管理员密码</target>
        </trans-unit>
        <trans-unit id="UseAdminPasswordCheckbox.Content" translate="yes" xml:space="preserve">
          <source>Login as Admin</source>
          <target state="translated">以管理员身份登录</target>
        </trans-unit>
        <trans-unit id="SettingsButton.ToolTipService.ToolTip" translate="yes" xml:space="preserve">
          <source>Settings</source>
          <target state="translated">设定</target>
        </trans-unit>
        <trans-unit id="PasswordDialog.PrimaryButtonText" translate="yes" xml:space="preserve">
          <source>Continue</source>
          <target state="translated">继续</target>
        </trans-unit>
        <trans-unit id="PasswordDialog.SecondaryButtonText" translate="yes" xml:space="preserve">
          <source>Cancel</source>
          <target state="translated">取消</target>
        </trans-unit>
        <trans-unit id="PasswordDialog.Title" translate="yes" xml:space="preserve">
          <source>Password Required</source>
          <target state="translated">输入密码</target>
        </trans-unit>
        <trans-unit id="PasswordDialogDescription.Text" translate="yes" xml:space="preserve">
          <source>Ask the server owner for the password if you don't have it.</source>
          <target state="translated">如果您没有密码，请询问服务器所有者。</target>
        </trans-unit>
        <trans-unit id="AppName" translate="yes" xml:space="preserve">
          <source>Nova Chat Client</source>
          <target state="translated">Nova 聊天客户</target>
        </trans-unit>
        <trans-unit id="MessageInput.PlaceholderText" translate="yes" xml:space="preserve">
          <source>Type a Message</source>
          <target state="translated">输入讯息</target>
        </trans-unit>
        <trans-unit id="SendButton.ToolTipService.ToolTip" translate="yes" xml:space="preserve">
          <source>Send</source>
          <target state="translated">发送</target>
        </trans-unit>
        <trans-unit id="VersionLabel" translate="yes" xml:space="preserve">
          <source>Nova Chat Client Version</source>
          <target state="translated">Nova 聊天客户 版本</target>
        </trans-unit>
        <trans-unit id="BackButton.ToolTipService.ToolTip" translate="yes" xml:space="preserve">
          <source>Go Back</source>
          <target state="translated">返回</target>
        </trans-unit>
        <trans-unit id="LeaveChatButton.ToolTipService.ToolTip" translate="yes" xml:space="preserve">
          <source>Leave Chat</source>
          <target state="translated">关闭聊天</target>
        </trans-unit>
        <trans-unit id="DisconnectDialog.PrimaryButtonText" translate="yes" xml:space="preserve">
          <source>Yes</source>
          <target state="new">Yes</target>
        </trans-unit>
        <trans-unit id="DisconnectDialog.SecondaryButtonText" translate="yes" xml:space="preserve">
          <source>No</source>
          <target state="new">No</target>
        </trans-unit>
        <trans-unit id="DisconnectDialog.Title" translate="yes" xml:space="preserve">
          <source>Confirm</source>
          <target state="new">Confirm</target>
        </trans-unit>
        <trans-unit id="DisconnectDialogDescription.Text" translate="yes" xml:space="preserve">
          <source>Are you sure you want to disconnect from the server?</source>
          <target state="new">Are you sure you want to disconnect from the server?</target>
        </trans-unit>
      </group>
    </body>
  </file>
